        <BitmapImage x:Key="PlainsBackground" CreateOptions="IgnoreImageCache" CacheOption="OnLoad" UriSource="PlainsBackground.jpg"/>
        <BitmapImage x:Key="Forest" CreateOptions="IgnoreImageCache" CacheOption="OnLoad" UriSource="Forest.jpg"/>
